{
  "arrowParens": "always",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "semi": false,
  "experimentalTernaries": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "quoteProps": "consistent",
  "trailingComma": "es5",
  "singleAttributePerLine": false,
  "htmlWhitespaceSensitivity": "ignore",
  "proseWrap": "preserve",
  "printWidth": 110,
  "tabWidth": 2,
  "useTabs": false,
  "embeddedLanguageFormatting": "off"
}